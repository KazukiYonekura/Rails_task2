<div class="container-field">
  <h4 class="search-result">検索結果:<%= @count %>件</h4>
  <div class="flex-container">
    <% @search_key.each do |key| %>
  	<div class="flex-item">
  	  <div class="result-frame">
  	    <a href="<%= "/posts/#{key.id}" %>">
  	  	  <div class="flex-image-box">
  	  	    <img src="/assets/room01.jpg" class="room-image">
  	  	  </div>
  	  	  <div class="room-profile">
  	  	    <p class="room-profile-name"><%= key.name %></p>
  	  	    <p class="room-profile-eria"><%= key.eria %></p>
  	  	    <p class="room-profile-price">¥<%= key.price %></p>
  	  	  </div>
  	  	</a>
  	  </div>
  	</div>
    <% end %>
  </div>
</div>